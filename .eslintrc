{
	"extends": "plugin:nullstack/recommended",
	"rules": {
		"nullstack/prettier": [
			"warn",
			{
				"allowIndentationTabs": true,
				"trailingComma": "all",
				"tabWidth": 2,
				"semi": false,
				"useTabs": true,
				"singleQuote": true,
				"printWidth": 120
			},
			{
				"usePrettierrc": false
			}
		]
	}
}